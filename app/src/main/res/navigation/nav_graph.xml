<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/nav_graph"
            app:startDestination="@id/FirstFragment">

    <fragment
            android:id="@+id/FirstFragment"
            android:name="com.example.vitaltime.FirstFragment"
            android:label="@string/first_fragment_label"
            tools:layout="@layout/fragment_first"/>
    <fragment
            android:id="@+id/SecondFragment"
            android:name="com.example.vitaltime.SecondFragment"
            android:label="@string/second_fragment_label"
            tools:layout="@layout/fragment_second"/>
    <fragment
        android:id="@+id/moodAndJournal"
        android:name="com.example.vitaltime.moodAndJournal"
        android:label="Diary"
        tools:layout="@layout/mood_journal">
        <action android:id="@+id/action_moodAndJournal_to_journalHome" app:destination="@id/journalHome"/>
    </fragment>
    <fragment android:id="@+id/journalHome" android:name="com.example.vitaltime.JournalHome"
              android:label="fragment_journal_home" tools:layout="@layout/fragment_journal_home">
        <action android:id="@+id/action_journalHome_to_moodAndJournal" app:destination="@id/moodAndJournal"/>
    </fragment>
</navigation>